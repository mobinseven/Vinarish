{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "persian-datepicker@1.1.3",
      "destination": "wwwroot/lib/persian-datepicker/",
      "files": [
        "assets/persian-date.min.js",
        "dist/js/persian-datepicker.min.js",
        "dist/css/persian-datepicker.min.css"
      ]
    },
    {
      "library": "font-awesome@5.8.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "popper.min.js"
      ]
    }
  ]
}