@model IEnumerable<VinarishMvc.Models.Report>
<div class="table-responsive">
    <table class="table">
        @if (Model.Count() == 0)
        {
            <tr>بدون گزارش</tr>
        }
        @foreach (Report r in Model)
        {
            @await Html.PartialAsync("~/Views/Reports/_ProgressReport.cshtml", r)
        }
    </table>
</div>