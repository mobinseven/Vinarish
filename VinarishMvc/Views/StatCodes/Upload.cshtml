
@*@model VinarishMvc.Models.StatCode*@

@{
    ViewData["Title"] = "افزودن کد وضعیت";
}
@model VinarishMvc.Views.StatCodes.UploadViewModel
<h1>افزودن</h1>

<h4>افزودن کدها از برگه</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Upload" method="post"
              enctype="multipart/form-data">
            <input type="file" name="file" class="btn btn-primary" />

            <div class="form-group">
                <label asp-for="CatId" class="control-label"></label>
                <select asp-for="CatId" class="form-control" asp-items="ViewBag.CatId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="افزودن کدها" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-secondary" role="button">بازگشت به فهرست</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
