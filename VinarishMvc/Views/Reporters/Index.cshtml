@model IEnumerable<VinarishMvc.Models.Reporter>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Department.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ReporterId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ReporterId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ReporterId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
